
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_LayoutGCHome.cshtml";
}
<script src="~/Scripts/bootstrap.min.js"></script>
<label style="position:absolute;top:420px;left:42%;z-index:5;color:white">欢迎使用GC家园登记系统</label>
<div id="myCarousel" class="carousel slide" style="z-index:1">
    <!-- 轮播（Carousel）指标 -->
    
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <!-- 轮播（Carousel）项目 -->
    <div class="carousel-inner" >
        <div class="item active">
            <img src="~/images/nbclass/img-39d37cbd3b796db2684fcae6e66c4702.jpg" alt="First slide">
        </div>
        <div class="item">
            <img src="~/images/nbclass/img-6d2d8d2f18ba59c45285057940b2249f.jpg" alt="Second slide">
        </div>
        <div class="item">
            <img src="~/images/nbclass/img-80bcfeeeebd97131748dff7e53feca03.jpg" alt="Third slide" >
        </div>
    </div>
    <!-- 轮播（Carousel）导航 -->
    <a class="carousel-control left" href="#myCarousel" data-slide="prev"> <span _ngcontent-c3="" aria-hidden="true" class="glyphicon glyphicon-chevron-left"></span></a>
    <a class="carousel-control right" href="#myCarousel" data-slide="next"> <span _ngcontent-c3="" aria-hidden="true" class="glyphicon glyphicon-chevron-right"></span></a>
</div>

