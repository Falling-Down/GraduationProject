
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MoveintoFun</title>
</head>
<body>
    <div style="width:50%;height:auto;margin:70px auto">
        <form class="form-horizontal" role="form">
            <div class="form-group">
                <label for="firstname" class="col-sm-2 control-label">学生学号</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" id="firstname" placeholder="请输入学生学号">
                </div>
            </div>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">楼宇</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" id="lastname" placeholder="请输入楼宇">
                </div>
            </div>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">宿舍</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" id="lastname" placeholder="请输入宿舍">
                </div>
            </div>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">入住时间</label>
                <div class="col-sm-9">
                    <input type="date" class="form-control" id="lastname" placeholder="请输入宿舍">
                </div>
            </div>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">登记人</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" id="lastname" placeholder="请输入登记人">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-4 col-sm-10">
                    <button type="submit" class="btn btn-default">提交</button>
                </div>
            </div>
        </form>
    </div>

</body>
</html>
