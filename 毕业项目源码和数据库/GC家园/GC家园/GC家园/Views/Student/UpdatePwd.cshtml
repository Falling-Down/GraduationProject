
@{
    ViewBag.Title = "UpdatePwd";
    Layout = "~/Views/Shared/_LayoutAdminGCHome.cshtml";
}

    <div style="width:50%;height:auto;margin:70px auto">
        <form class="form-horizontal" role="form" action="/Student/" id="AddValidate">
            <div class="form-group">
                <label for="firstname" class="col-sm-2 control-label">账号</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" name="StuNumber" placeholder="请输入学生学号" onblur="nbclass()" id="StuNumber" value="@ViewBag.StuNumber">
                    <p id="content" style="color:red"></p>
                </div>
            </div>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">旧密码</label>
                <div class="col-sm-9">
                    <select class="form-control" id="DormID" name="DormID">
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">新密码</label>
                <div class="col-sm-9">
                    <input type="date" class="form-control" name="MoveintoTime" placeholder="请输入宿舍">
                </div>
            </div>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">再次输入密码</label>
                <div class="col-sm-9">
                    <input type="text" class="form-control" name="MoveintoPeople" placeholder="请输入登记人">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-4 col-sm-10">
                    <button type="submit" class="btn btn-default">提交</button>
                </div>
            </div>
        </form>
    </div>

