
@{
    ViewBag.Title = "AddMoveout";
    Layout = "~/Views/Shared/_LayoutGCHome.cshtml";
}

    <div style="width:50%;height:auto;margin:70px auto">
        <form class="form-horizontal" role="form" action="/Student/AddMoveout" method="post">
            <input type="hidden" name="StuID" value="@ViewBag.ID" id="StuID"/>
            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">迁出日期</label>
                <div class="col-sm-9">
                    <input type="date" class="form-control" id="MoveoutDate" name="MoveoutDate" />
                </div>
            </div>

            <div class="form-group">
                <label for="lastname" class="col-sm-2 control-label">备注</label>
                <div class="col-sm-9">
                    <textarea class="form-control" name="MoveoutMark" id="MoveoutMark" style="height:150px"></textarea>
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-offset-4 col-sm-10">
                    <button type="submit" class="btn btn-default">提交</button>
                    <a href="/Student/Moveout" class="btn btn-default">返回</a>
                </div>
            </div>
        </form>
    </div>

